{
  "openapi": "3.0.4",
  "info": {
    "title": "University_system",
    "version": "1.0"
  },
  "paths": {
    "/Get Student Balance by id": {
      "get": {
        "tags": [
          "Finance"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/id": {
      "put": {
        "tags": [
          "Finance"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "money",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "get": {
        "tags": [
          "Student"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/register year": {
      "get": {
        "tags": [
          "Student"
        ],
        "parameters": [
          {
            "name": "year",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Update student data": {
      "put": {
        "tags": [
          "Student"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Student"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Student"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Student"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Material": {
        "required": [
          "name",
          "number_of_hour",
          "subject_professor"
        ],
        "type": "object",
        "properties": {
          "materialId": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "number_of_hour": {
            "type": "integer",
            "format": "int32"
          },
          "completion_requires": {
            "type": "string",
            "nullable": true
          },
          "subject_professor": {
            "minLength": 1,
            "type": "string"
          },
          "students": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Student"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Student": {
        "required": [
          "first_Name",
          "gender",
          "last_Name",
          "year_of_registration"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "userName": {
            "type": "string",
            "nullable": true
          },
          "normalizedUserName": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "normalizedEmail": {
            "type": "string",
            "nullable": true
          },
          "emailConfirmed": {
            "type": "boolean"
          },
          "passwordHash": {
            "type": "string",
            "nullable": true
          },
          "securityStamp": {
            "type": "string",
            "nullable": true
          },
          "concurrencyStamp": {
            "type": "string",
            "nullable": true
          },
          "phoneNumber": {
            "type": "string",
            "nullable": true
          },
          "phoneNumberConfirmed": {
            "type": "boolean"
          },
          "twoFactorEnabled": {
            "type": "boolean"
          },
          "lockoutEnd": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "lockoutEnabled": {
            "type": "boolean"
          },
          "accessFailedCount": {
            "type": "integer",
            "format": "int32"
          },
          "first_Name": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "last_Name": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "gender": {
            "maxLength": 20,
            "minLength": 1,
            "type": "string"
          },
          "year_of_registration": {
            "type": "integer",
            "format": "int32"
          },
          "gpa": {
            "type": "number",
            "format": "double"
          },
          "is_Grant": {
            "type": "boolean"
          },
          "student_Balance": {
            "type": "integer",
            "format": "int32"
          },
          "materials": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Material"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}